#include<stdio.h>
int main()
{
    int i,m,n;
    printf("Enter the value of m:");
    scanf("%d",&m);
    
    printf("Enter the value of n:");
    scanf("%d",&n);
    for(i=m;i<=n;i++)
    {
        if (i%2==0)
        printf("\n%d is an even number:",i);
        else 
        printf("\n%d is an odd number:",i);
        
    }
}
